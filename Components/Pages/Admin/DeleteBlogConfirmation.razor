@inject ISnackbar Snackbar

<MudDialog>
    <DialogContent>
        Are you sure you want to delete this blog post?
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">No</MudButton>
        <MudButton Color="Color.Primary" OnClick="DeleteBlog">Yes</MudButton>
    </DialogActions>
</MudDialog>
